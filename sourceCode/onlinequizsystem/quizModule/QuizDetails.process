<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:ns2="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd8" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.eniquesolution.com/ESI_integartion/CommonFramework" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd9">
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd9" schemaLocation="/schemas/quizDetailsResponse.xsd"/>
    <pd:name>quizModule/QuizDetails.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startX>150</pd:startX>
    <pd:startY>62</pd:startY>
    <pd:returnBindings>
        <xsl:choose>
            <xsl:when test="exists($_error/ns1:ErrorReport/Msg)">
                <pfx:QuizDetailsResponse>
                    <pfx:responseCode>
                        <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                    </pfx:responseCode>
                </pfx:QuizDetailsResponse>
            </xsl:when>
            <xsl:otherwise>
                <xsl:choose>
                    <xsl:when test="count($JDBC-Query/resultSet/Record)>0">
                        <pfx:QuizDetailsResponse>
                            <pfx:responseCode>
                                <xsl:value-of select="200"/>
                            </pfx:responseCode>
                            <xsl:for-each select="$JDBC-Query/resultSet/Record">
                                <ns2:quizDetails>
                                    <ns2:quizid>
                                        <xsl:value-of select="QUIZ_ID"/>
                                    </ns2:quizid>
                                    <ns2:quizname>
                                        <xsl:value-of select="QUIZ_NAME"/>
                                    </ns2:quizname>
                                    <ns2:quizdate>
                                        <xsl:value-of select="QUIZ_DATE"/>
                                    </ns2:quizdate>
                                    <ns2:totalquestions>
                                        <xsl:value-of select="TOTAL_QUESTIONS"/>
                                    </ns2:totalquestions>
                                    <ns2:totalparticipants>
                                        <xsl:value-of select="TOTAL_PARTICIPANTS"/>
                                    </ns2:totalparticipants>
                                    <ns2:totalvacancies>
                                        <xsl:value-of select="TOTAL_VACANCIES"/>
                                    </ns2:totalvacancies>
                                    <ns2:totalrounds>
                                        <xsl:value-of select="TOTALROUNDS"/>
                                    </ns2:totalrounds>
                                </ns2:quizDetails>
                            </xsl:for-each>
                        </pfx:QuizDetailsResponse>
                    </xsl:when>
                    <xsl:otherwise>
                        <pfx:QuizDetailsResponse>
                            <pfx:responseCode>
                                <xsl:value-of select="100"/>
                            </pfx:responseCode>
                        </pfx:QuizDetailsResponse>
                    </xsl:otherwise>
                </xsl:choose>
            </xsl:otherwise>
        </xsl:choose>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx:QuizDetailsResponse"/>
    <pd:endX>640</pd:endX>
    <pd:endY>61</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1681806288649</pd:targetNamespace>
    <pd:activity name="JDBC Query">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>382</pd:x>
        <pd:y>59</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/resources/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <statement>SELECT *
FROM QUIZ_DETAILS
WHERE QUIZ_DATE &gt;= TRUNC(SYSDATE)</statement>
            <QueryOutputCachedSchemaColumns>QUIZ_ID</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>2</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>NUMBER</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>QUIZ_NAME</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>QUIZ_DATE</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>93</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>DATE</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>TOTAL_QUESTIONS</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>2</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>NUMBER</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>TOTAL_PARTICIPANTS</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>2</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>NUMBER</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>TOTAL_VACANCIES</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>2</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>NUMBER</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>TOTALROUNDS</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>2</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>NUMBER</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <oraObjects/>
            <oraTables/>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput/>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>388</pd:x>
        <pd:y>234</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="startLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>274</pd:x>
        <pd:y>135</pd:y>
        <config>
            <processName>/processes/infoLog.process</processName>
        </config>
        <pd:inputBindings>
            <ns:LogExceptionHandler>
                <ns:ApplictionName>
                    <xsl:value-of select="'quizModule'"/>
                </ns:ApplictionName>
                <ns:ProcessName>
                    <xsl:value-of select="'quizDetails'"/>
                </ns:ProcessName>
                <ns:LogType>
                    <xsl:value-of select="'startLog'"/>
                </ns:LogType>
                <ns:status>
                    <xsl:value-of select="'success'"/>
                </ns:status>
                <ns:CorrelationID>
                    <xsl:value-of select="''"/>
                </ns:CorrelationID>
                <ns:MessageType>
                    <xsl:value-of select="'log message'"/>
                </ns:MessageType>
                <ns:Timestamp>
                    <xsl:value-of select="tib:timestamp()"/>
                </ns:Timestamp>
            </ns:LogExceptionHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="endLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>492</pd:x>
        <pd:y>130</pd:y>
        <config>
            <processName>/processes/infoLog.process</processName>
        </config>
        <pd:inputBindings>
            <ns:LogExceptionHandler>
                <ns:ApplictionName>
                    <xsl:value-of select="'quizModule'"/>
                </ns:ApplictionName>
                <ns:ProcessName>
                    <xsl:value-of select="'quizDetails'"/>
                </ns:ProcessName>
                <ns:LogType>
                    <xsl:value-of select="'end log'"/>
                </ns:LogType>
                <ns:status>
                    <xsl:value-of select="'success'"/>
                </ns:status>
                <ns:CorrelationID>
                    <xsl:value-of select="''"/>
                </ns:CorrelationID>
                <ns:MessageType>
                    <xsl:value-of select="'log message'"/>
                </ns:MessageType>
                <ns:Timestamp>
                    <xsl:value-of select="tib:timestamp()"/>
                </ns:Timestamp>
                <ns:PAYLOAD>
                    <xsl:value-of select="tib:render-xml($JDBC-Query/resultSet)"/>
                </ns:PAYLOAD>
            </ns:LogExceptionHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="errorLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>516</pd:x>
        <pd:y>222</pd:y>
        <config>
            <processName>/processes/errorLog.process</processName>
        </config>
        <pd:inputBindings>
            <ns:ErrorLogHandler>
                <ns:ApplictionName>
                    <xsl:value-of select="'quizModule'"/>
                </ns:ApplictionName>
                <ns:ProcessName>
                    <xsl:value-of select="'quizDetails'"/>
                </ns:ProcessName>
                <ns:LogType>
                    <xsl:value-of select="'error log'"/>
                </ns:LogType>
                <ns:status>
                    <xsl:value-of select="'failed'"/>
                </ns:status>
                <ns:CorrelationID>
                    <xsl:value-of select="''"/>
                </ns:CorrelationID>
                <ns:MessageType>
                    <xsl:value-of select="'error message'"/>
                </ns:MessageType>
                <ns:Timestamp>
                    <xsl:value-of select="tib:timestamp()"/>
                </ns:Timestamp>
                <xsl:if test="$_error/ns1:ErrorReport/MsgCode">
                    <ns:ErrorCode>
                        <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                    </ns:ErrorCode>
                </xsl:if>
                <xsl:if test="$_error/ns1:ErrorReport/Msg">
                    <ns:ErrorMsg>
                        <xsl:value-of select="$_error/ns1:ErrorReport/Msg"/>
                    </ns:ErrorMsg>
                </xsl:if>
            </ns:ErrorLogHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>startLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>JDBC Query</pd:from>
        <pd:to>endLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>startLog</pd:from>
        <pd:to>JDBC Query</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>endLog</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>errorLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>errorLog</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>