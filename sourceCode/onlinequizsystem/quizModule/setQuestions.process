<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.eniquesolution.com/ESI_integartion/CommonFramework" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd6">
    <xsd:import namespace="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd6" schemaLocation="/schemas/response.xsd"/>
    <pd:name>quizModule/setQuestions.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="payload">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="quizID" type="xsd:string"/>
                    <xsd:element name="rounds" type="xsd:int"/>
                    <xsd:element name="noOfQuestions" type="xsd:int"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>86</pd:startX>
    <pd:startY>84</pd:startY>
    <pd:returnBindings>
        <xsl:choose>
            <xsl:when test="exists($_error/ns1:ErrorReport/Msg)">
                <pfx:response>
                    <pfx:responseMessage>
                        <xsl:value-of select="$_error/ns1:ErrorReport/Msg"/>
                    </pfx:responseMessage>
                    <pfx:responseCode>
                        <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                    </pfx:responseCode>
                </pfx:response>
            </xsl:when>
            <xsl:otherwise>
                <xsl:choose>
                    <xsl:when test="contains(tib:concat-sequence-format($AccumulateUpdate/AccumulatedOutput/jdbcUpdateActivityOutput, ', '), '1')">
                        <pfx:response>
                            <pfx:responseMessage>
                                <xsl:value-of select="&quot;questions successfull set&quot;"/>
                            </pfx:responseMessage>
                            <pfx:responseCode>
                                <xsl:value-of select="200"/>
                            </pfx:responseCode>
                        </pfx:response>
                    </xsl:when>
                    <xsl:otherwise>
                        <pfx:response>
                            <pfx:responseMessage>
                                <xsl:value-of select="'questions failed to set'"/>
                            </pfx:responseMessage>
                            <pfx:responseCode>
                                <xsl:value-of select="100"/>
                            </pfx:responseCode>
                        </pfx:response>
                    </xsl:otherwise>
                </xsl:choose>
            </xsl:otherwise>
        </xsl:choose>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx:response"/>
    <pd:endX>988</pd:endX>
    <pd:endY>87</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1681902256730</pd:targetNamespace>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>375</pd:x>
        <pd:y>5</pd:y>
        <pd:width>289</pd:width>
        <pd:height>140</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>repeat</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:indexSlot>i</pd:indexSlot>
            <pd:outSlot>AccumulateUpdate</pd:outSlot>
            <pd:activityOutputName>JDBC Update</pd:activityOutputName>
            <pd:repeatCondition>$i&gt;$Start/payload/rounds</pd:repeatCondition>
            <pd:accumulateOutput>true</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>JDBC Query</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>JDBC Update</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>JDBC Query</pd:from>
            <pd:to>JDBC Update</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="JDBC Query">
            <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
            <pd:x>433</pd:x>
            <pd:y>69</pd:y>
            <config>
                <timeout>10</timeout>
                <commit>false</commit>
                <maxRows>100</maxRows>
                <emptyStrAsNil>false</emptyStrAsNil>
                <jdbcSharedConfig>/resources/JDBC Connection.sharedjdbc</jdbcSharedConfig>
                <statement>SELECT * FROM (
  SELECT * FROM QUIZQUESTIONS ORDER BY dbms_random.value
)
WHERE ROWNUM &lt;= ?</statement>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>ROWNUM</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                </Prepared_Param_DataType>
                <QueryOutputCachedSchemaColumns>QUESTIONID</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>2</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaDataTypesName>NUMBER</QueryOutputCachedSchemaDataTypesName>
                <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
                <QueryOutputCachedSchemaColumns>QUESTIONTEXT</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
                <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
                <QueryOutputCachedSchemaColumns>ANSWER</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
                <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
                <QueryOutputCachedSchemaColumns>OPTIONA</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
                <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
                <QueryOutputCachedSchemaColumns>OPTIONB</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
                <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
                <QueryOutputCachedSchemaColumns>OPTIONC</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
                <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
                <QueryOutputCachedSchemaColumns>OPTIOND</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
                <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
                <oraObjects/>
                <oraTables/>
            </config>
            <pd:inputBindings>
                <jdbcQueryActivityInput>
                    <ROWNUM>
                        <xsl:value-of select="$Start/payload/noOfQuestions"/>
                    </ROWNUM>
                </jdbcQueryActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="JDBC Update">
            <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
            <pd:x>578</pd:x>
            <pd:y>69</pd:y>
            <config>
                <timeout>10</timeout>
                <commit>false</commit>
                <emptyStrAsNil>false</emptyStrAsNil>
                <jdbcSharedConfig>/resources/JDBC Connection.sharedjdbc</jdbcSharedConfig>
                <statement>INSERT INTO QUESTIONS (QUIZID, ROUND, QUESTIONTEXT, ANSWER, OPTIONA, OPTIONB, OPTIONC, OPTIOND)
VALUES (?, ?, ?, ?, ?, ?,?, ?)</statement>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>QUIZID</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>ROUND</parameterName>
                        <dataType>INTEGER</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>QUESTIONTEXT</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>ANSWER</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>OPTIONA</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>OPTIONB</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>OPTIONC</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>OPTIOND</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                </Prepared_Param_DataType>
                <batchUpdate>true</batchUpdate>
            </config>
            <pd:inputBindings>
                <jdbcUpdateActivityInput>
                    <xsl:for-each select="$JDBC-Query/resultSet/Record">
                        <Record>
                            <QUIZID>
                                <xsl:value-of select="$Start/payload/quizID"/>
                            </QUIZID>
                            <ROUND>
                                <xsl:value-of select="$i"/>
                            </ROUND>
                            <QUESTIONTEXT>
                                <xsl:value-of select="QUESTIONTEXT"/>
                            </QUESTIONTEXT>
                            <ANSWER>
                                <xsl:value-of select="ANSWER"/>
                            </ANSWER>
                            <OPTIONA>
                                <xsl:value-of select="OPTIONA"/>
                            </OPTIONA>
                            <OPTIONB>
                                <xsl:value-of select="OPTIONB"/>
                            </OPTIONB>
                            <OPTIONC>
                                <xsl:value-of select="OPTIONC"/>
                            </OPTIONC>
                            <OPTIOND>
                                <xsl:value-of select="OPTIOND"/>
                            </OPTIOND>
                        </Record>
                    </xsl:for-each>
                </jdbcUpdateActivityInput>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>476</pd:x>
        <pd:y>410</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="startLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>269</pd:x>
        <pd:y>191</pd:y>
        <config>
            <processName>/processes/infoLog.process</processName>
        </config>
        <pd:inputBindings>
            <ns:LogExceptionHandler>
                <ns:ApplictionName>
                    <xsl:value-of select="'quizModule'"/>
                </ns:ApplictionName>
                <ns:ProcessName>
                    <xsl:value-of select="'setQuestions'"/>
                </ns:ProcessName>
                <ns:LogType>
                    <xsl:value-of select="'startLog'"/>
                </ns:LogType>
                <ns:status>
                    <xsl:value-of select="'success'"/>
                </ns:status>
                <ns:CorrelationID>
                    <xsl:value-of select="''"/>
                </ns:CorrelationID>
                <ns:MessageType>
                    <xsl:value-of select="'log message'"/>
                </ns:MessageType>
                <ns:Timestamp>
                    <xsl:value-of select="tib:timestamp()"/>
                </ns:Timestamp>
                <ns:PAYLOAD>
                    <xsl:value-of select="concat($Start/payload/quizID,', ',$Start/payload/rounds)"/>
                </ns:PAYLOAD>
            </ns:LogExceptionHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="endLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>835</pd:x>
        <pd:y>200</pd:y>
        <config>
            <processName>/processes/infoLog.process</processName>
        </config>
        <pd:inputBindings>
            <ns:LogExceptionHandler>
                <ns:ApplictionName>
                    <xsl:value-of select="'quizModule'"/>
                </ns:ApplictionName>
                <ns:ProcessName>
                    <xsl:value-of select="'setQuestions'"/>
                </ns:ProcessName>
                <ns:LogType>
                    <xsl:value-of select="'endLog'"/>
                </ns:LogType>
                <ns:status>
                    <xsl:value-of select="'success'"/>
                </ns:status>
                <ns:CorrelationID>
                    <xsl:value-of select="''"/>
                </ns:CorrelationID>
                <ns:MessageType>
                    <xsl:value-of select="'log message'"/>
                </ns:MessageType>
                <ns:Timestamp>
                    <xsl:value-of select="tib:timestamp()"/>
                </ns:Timestamp>
                <ns:PAYLOAD>
                    <xsl:value-of select="tib:concat-sequence-format($AccumulateUpdate/AccumulatedOutput/jdbcUpdateActivityOutput,', ')"/>
                </ns:PAYLOAD>
            </ns:LogExceptionHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="errorLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>808</pd:x>
        <pd:y>411</pd:y>
        <config>
            <processName>/processes/errorLog.process</processName>
        </config>
        <pd:inputBindings>
            <ns:ErrorLogHandler>
                <ns:ApplictionName>
                    <xsl:value-of select="'quizModule'"/>
                </ns:ApplictionName>
                <ns:ProcessName>
                    <xsl:value-of select="'setQuestions'"/>
                </ns:ProcessName>
                <ns:LogType>
                    <xsl:value-of select="'error log'"/>
                </ns:LogType>
                <ns:status>
                    <xsl:value-of select="'failed'"/>
                </ns:status>
                <ns:CorrelationID>
                    <xsl:value-of select="''"/>
                </ns:CorrelationID>
                <ns:MessageType>
                    <xsl:value-of select="'error message'"/>
                </ns:MessageType>
                <ns:Timestamp>
                    <xsl:value-of select="tib:timestamp()"/>
                </ns:Timestamp>
                <xsl:if test="$_error/ns1:ErrorReport/MsgCode">
                    <ns:ErrorCode>
                        <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                    </ns:ErrorCode>
                </xsl:if>
                <xsl:if test="$_error/ns1:ErrorReport/Msg">
                    <ns:ErrorMsg>
                        <xsl:value-of select="$_error/ns1:ErrorReport/Msg"/>
                    </ns:ErrorMsg>
                </xsl:if>
            </ns:ErrorLogHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>startLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>endLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>startLog</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>endLog</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>errorLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>errorLog</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>