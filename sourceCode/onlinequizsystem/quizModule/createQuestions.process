<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx3="http://www.eniquesolution.com/ESI_integartion/CommonFramework" xmlns:pfx2="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd6" xmlns:pfx="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd11">
    <xsd:import namespace="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd11" schemaLocation="/schemas/createQuestions.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd6" schemaLocation="/schemas/response.xsd"/>
    <pd:name>quizModule/createQuestions.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="payload" type="xsd:string"/>
    </pd:startType>
    <pd:startX>66</pd:startX>
    <pd:startY>65</pd:startY>
    <pd:returnBindings>
        <pfx2:response>
            <pfx2:responseMessage>
                <xsl:value-of select="200"/>
            </pfx2:responseMessage>
            <pfx2:responseCode>
                <xsl:value-of select="&quot;&quot;"/>
            </pfx2:responseCode>
        </pfx2:response>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx2:response"/>
    <pd:endX>768</pd:endX>
    <pd:endY>59</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1681892398508</pd:targetNamespace>
    <pd:activity name="Parse JSON">
        <pd:type>com.tibco.plugin.json.activities.JSONParserActivity</pd:type>
        <pd:resourceType>ae.activities.JSONParserActivity</pd:resourceType>
        <pd:x>314</pd:x>
        <pd:y>64</pd:y>
        <config>
            <SchemaType>xsdType</SchemaType>
            <BadgerfishCheck>false</BadgerfishCheck>
            <ActivityOutputEditor ref="pfx:questions"/>
        </config>
        <pd:inputBindings>
            <ns:ActivityInputClass>
                <jsonString>
                    <xsl:value-of select="$Start/payload"/>
                </jsonString>
            </ns:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="JDBC Update">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>484</pd:x>
        <pd:y>64</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/resources/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <statement>INSERT INTO QUIZ_QUESTIONS (QUESTION_TEXT, CORRECT_ANSWER, OPTION_A, OPTION_B, OPTION_C, OPTION_D)
VALUES (?, ?, ?,?, ?, ?)</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>QUESTIONTEXT</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>ANSWER</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>OPTIONA</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>OPTIONB</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>OPTIONC</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>OPTIOND</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <batchUpdate>true</batchUpdate>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <xsl:for-each select="$Parse-JSON/ns:ActivityOutputClass/pfx:questions/pfx:question">
                    <Record>
                        <QUESTIONTEXT>
                            <xsl:value-of select="pfx:questiontext"/>
                        </QUESTIONTEXT>
                        <ANSWER>
                            <xsl:value-of select="pfx:answer"/>
                        </ANSWER>
                        <OPTIONA>
                            <xsl:value-of select="pfx:optionA"/>
                        </OPTIONA>
                        <OPTIONB>
                            <xsl:value-of select="pfx:optionB"/>
                        </OPTIONB>
                        <OPTIONC>
                            <xsl:value-of select="pfx:optionC"/>
                        </OPTIONC>
                        <OPTIOND>
                            <xsl:value-of select="pfx:optionD"/>
                        </OPTIOND>
                    </Record>
                </xsl:for-each>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="startLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>192</pd:x>
        <pd:y>153</pd:y>
        <config>
            <processName>/processes/infoLog.process</processName>
        </config>
        <pd:inputBindings>
            <pfx3:LogExceptionHandler>
                <pfx3:ApplictionName>
                    <xsl:value-of select="'quizModule'"/>
                </pfx3:ApplictionName>
                <pfx3:ProcessName>
                    <xsl:value-of select="'createQuestions'"/>
                </pfx3:ProcessName>
                <pfx3:LogType>
                    <xsl:value-of select="'startlog'"/>
                </pfx3:LogType>
                <pfx3:status>
                    <xsl:value-of select="'success'"/>
                </pfx3:status>
                <pfx3:CorrelationID>
                    <xsl:value-of select="''"/>
                </pfx3:CorrelationID>
                <pfx3:MessageType>
                    <xsl:value-of select="'log message'"/>
                </pfx3:MessageType>
                <pfx3:Timestamp>
                    <xsl:value-of select="tib:timestamp()"/>
                </pfx3:Timestamp>
                <pfx3:PAYLOAD>
                    <xsl:value-of select="$Start/payload"/>
                </pfx3:PAYLOAD>
            </pfx3:LogExceptionHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="errorLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>626</pd:x>
        <pd:y>275</pd:y>
        <config>
            <processName>/processes/errorLog.process</processName>
        </config>
        <pd:inputBindings>
            <pfx3:ErrorLogHandler>
                <pfx3:ApplictionName>
                    <xsl:value-of select="'quizModule'"/>
                </pfx3:ApplictionName>
                <pfx3:ProcessName>
                    <xsl:value-of select="'createQuestions'"/>
                </pfx3:ProcessName>
                <pfx3:LogType>
                    <xsl:value-of select="'errorLog'"/>
                </pfx3:LogType>
                <pfx3:status>
                    <xsl:value-of select="'Failed'"/>
                </pfx3:status>
                <pfx3:CorrelationID>
                    <xsl:value-of select="''"/>
                </pfx3:CorrelationID>
                <pfx3:MessageType>
                    <xsl:value-of select="'error message'"/>
                </pfx3:MessageType>
                <pfx3:Timestamp>
                    <xsl:value-of select="tib:timestamp()"/>
                </pfx3:Timestamp>
                <xsl:if test="$_error/ns1:ErrorReport/MsgCode">
                    <pfx3:ErrorCode>
                        <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                    </pfx3:ErrorCode>
                </xsl:if>
                <xsl:if test="$_error/ns1:ErrorReport/Msg">
                    <pfx3:ErrorMsg>
                        <xsl:value-of select="$_error/ns1:ErrorReport/Msg"/>
                    </pfx3:ErrorMsg>
                </xsl:if>
            </pfx3:ErrorLogHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="endLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>631</pd:x>
        <pd:y>157</pd:y>
        <config>
            <processName>/processes/infoLog.process</processName>
        </config>
        <pd:inputBindings>
            <pfx3:LogExceptionHandler>
                <pfx3:ApplictionName>
                    <xsl:value-of select="'quizModule'"/>
                </pfx3:ApplictionName>
                <pfx3:ProcessName>
                    <xsl:value-of select="'createQuestions'"/>
                </pfx3:ProcessName>
                <pfx3:LogType>
                    <xsl:value-of select="'endLog'"/>
                </pfx3:LogType>
                <pfx3:status>
                    <xsl:value-of select="'success'"/>
                </pfx3:status>
                <pfx3:CorrelationID>
                    <xsl:value-of select="''"/>
                </pfx3:CorrelationID>
                <pfx3:MessageType>
                    <xsl:value-of select="'endLog'"/>
                </pfx3:MessageType>
                <pfx3:Timestamp>
                    <xsl:value-of select="tib:timestamp()"/>
                </pfx3:Timestamp>
                <pfx3:PAYLOAD>
                    <xsl:value-of select="tib:render-xml($JDBC-Update/jdbcUpdateActivityOutput)"/>
                </pfx3:PAYLOAD>
            </pfx3:LogExceptionHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>402</pd:x>
        <pd:y>274</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>startLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse JSON</pd:from>
        <pd:to>JDBC Update</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>startLog</pd:from>
        <pd:to>Parse JSON</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>JDBC Update</pd:from>
        <pd:to>endLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>endLog</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>errorLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>errorLog</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>