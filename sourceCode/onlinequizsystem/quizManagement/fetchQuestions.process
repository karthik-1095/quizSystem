<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:ns="http://www.eniquesolution.com/ESI_integartion/CommonFramework" xmlns:pfx5="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd16" xmlns:pfx4="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd6" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx3="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd" xmlns:pfx2="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd12" xmlns:pfx="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd2">
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd2" schemaLocation="/schemas/createQuizQuestions.xsd"/>
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd12" schemaLocation="/schemas/getQuizQuestions.xsd"/>
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd" schemaLocation="/schemas/adminDetails.xsd"/>
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd6" schemaLocation="/schemas/response.xsd"/>
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd16" schemaLocation="/schemas/getQuizQuestion.xsd"/>
    <pd:name>quizManagement/fetchQuestions.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx2:quizQuestions"/>
    <pd:startX>150</pd:startX>
    <pd:startY>64</pd:startY>
    <pd:returnBindings>
        <pfx4:response>
            <pfx4:responseMessage>
                <xsl:value-of select="tib:render-xml($JDBC-Query/resultSet/Record[$Start/pfx2:quizQuestions/pfx2:questionID])"/>
            </pfx4:responseMessage>
            <pfx4:responseCode>
                <xsl:value-of select="200"/>
            </pfx4:responseCode>
        </pfx4:response>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx4:response"/>
    <pd:endX>729</pd:endX>
    <pd:endY>68</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1680082762229</pd:targetNamespace>
    <pd:activity name="JDBC Query">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>427</pd:x>
        <pd:y>63</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/resources/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <statement>SELECT * FROM (
  SELECT * FROM QUESTIONS where ROUND= ? and QUIZID=? ORDER BY dbms_random.value
)
WHERE ROWNUM &lt;= 2</statement>
            <QueryOutputCachedSchemaColumns>QUIZID</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>2</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>NUMBER</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>ROUND</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>2</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>NUMBER</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>QUESTIONTEXT</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>ANSWER</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>OPTIONA</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>OPTIONB</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>OPTIONC</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>OPTIOND</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>QUESTIONID</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <oraObjects/>
            <oraTables/>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>ROUND</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>QUIZID</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <ROUND>
                    <xsl:value-of select="$Start/pfx2:quizQuestions/pfx2:round"/>
                </ROUND>
                <QUIZID>
                    <xsl:value-of select="$Start/pfx2:quizQuestions/pfx2:quizID"/>
                </QUIZID>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="startLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>286</pd:x>
        <pd:y>137</pd:y>
        <config>
            <processName>/processes/infoLog.process</processName>
        </config>
        <pd:inputBindings>
            <ns:LogExceptionHandler>
                <ns:ApplictionName>
                    <xsl:value-of select="'quiz management'"/>
                </ns:ApplictionName>
                <ns:ProcessName>
                    <xsl:value-of select="$_processContext/ns1:ProcessContext/ProjectName"/>
                </ns:ProcessName>
                <ns:LogType>
                    <xsl:value-of select="'start log'"/>
                </ns:LogType>
                <ns:status>
                    <xsl:value-of select="'success'"/>
                </ns:status>
                <ns:CorrelationID>
                    <xsl:value-of select="''"/>
                </ns:CorrelationID>
                <ns:MessageType>
                    <xsl:value-of select="'request payload'"/>
                </ns:MessageType>
                <ns:Timestamp>
                    <xsl:value-of select="tib:timestamp()"/>
                </ns:Timestamp>
                <ns:PAYLOAD>
                    <xsl:value-of select="'fetch questions'"/>
                </ns:PAYLOAD>
            </ns:LogExceptionHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="endLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>661</pd:x>
        <pd:y>96</pd:y>
        <config>
            <processName>/processes/infoLog.process</processName>
        </config>
        <pd:inputBindings>
            <ns:LogExceptionHandler>
                <ns:ApplictionName>
                    <xsl:value-of select="'quiz management'"/>
                </ns:ApplictionName>
                <ns:ProcessName>
                    <xsl:value-of select="$_processContext/ns1:ProcessContext/ProjectName"/>
                </ns:ProcessName>
                <ns:LogType>
                    <xsl:value-of select="'endlog'"/>
                </ns:LogType>
                <ns:status>
                    <xsl:value-of select="'success'"/>
                </ns:status>
                <ns:CorrelationID>
                    <xsl:value-of select="''"/>
                </ns:CorrelationID>
                <ns:MessageType>
                    <xsl:value-of select="'response payload'"/>
                </ns:MessageType>
                <ns:Timestamp>
                    <xsl:value-of select="tib:timestamp()"/>
                </ns:Timestamp>
                <ns:PAYLOAD>
                    <xsl:value-of select="tib:render-xml($JDBC-Query/resultSet)"/>
                </ns:PAYLOAD>
            </ns:LogExceptionHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>447</pd:x>
        <pd:y>276</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="errorLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>621</pd:x>
        <pd:y>273</pd:y>
        <config>
            <processName>/processes/errorLog.process</processName>
        </config>
        <pd:inputBindings>
            <ns:ErrorLogHandler>
                <ns:ApplictionName>
                    <xsl:value-of select="'quiz management'"/>
                </ns:ApplictionName>
                <ns:ProcessName>
                    <xsl:value-of select="$_processContext/ns1:ProcessContext/ProjectName"/>
                </ns:ProcessName>
                <ns:LogType>
                    <xsl:value-of select="'error log'"/>
                </ns:LogType>
                <ns:status>
                    <xsl:value-of select="'failed'"/>
                </ns:status>
                <ns:CorrelationID>
                    <xsl:value-of select="''"/>
                </ns:CorrelationID>
                <ns:MessageType>
                    <xsl:value-of select="'error message'"/>
                </ns:MessageType>
                <ns:Timestamp>
                    <xsl:value-of select="tib:timestamp()"/>
                </ns:Timestamp>
                <xsl:if test="$_error/ns1:ErrorReport/MsgCode">
                    <ns:ErrorCode>
                        <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                    </ns:ErrorCode>
                </xsl:if>
                <xsl:if test="$_error/ns1:ErrorReport/Msg">
                    <ns:ErrorMsg>
                        <xsl:value-of select="$_error/ns1:ErrorReport/Msg"/>
                    </ns:ErrorMsg>
                </xsl:if>
            </ns:ErrorLogHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render JSON">
        <pd:type>com.tibco.plugin.json.activities.JSONRenderActivity</pd:type>
        <pd:resourceType>ae.activities.JSONRenderActivity</pd:resourceType>
        <pd:x>537</pd:x>
        <pd:y>127</pd:y>
        <config>
            <SchemaType>xsdType</SchemaType>
            <BadgerfishCheck>false</BadgerfishCheck>
            <ActivityInputEditor ref="pfx5:quiQuestion"/>
        </config>
        <pd:inputBindings>
            <ns2:ActivityInputClass>
                <xsl:for-each select="$JDBC-Query/resultSet/Record[$Start/pfx2:quizQuestions/pfx2:questionID]">
                    <pfx5:quiQuestion>
                        <pfx5:QUESTIONTEXT>
                            <xsl:value-of select="QUESTIONTEXT"/>
                        </pfx5:QUESTIONTEXT>
                        <pfx5:OPTIONA>
                            <xsl:value-of select="OPTIONA"/>
                        </pfx5:OPTIONA>
                        <pfx5:OPTIONB>
                            <xsl:value-of select="OPTIONB"/>
                        </pfx5:OPTIONB>
                        <pfx5:OPTIONC>
                            <xsl:value-of select="OPTIONC"/>
                        </pfx5:OPTIONC>
                        <pfx5:OPTIOND>
                            <xsl:value-of select="OPTIOND"/>
                        </pfx5:OPTIOND>
                    </pfx5:quiQuestion>
                </xsl:for-each>
            </ns2:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>startLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>JDBC Query</pd:from>
        <pd:to>Render JSON</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>startLog</pd:from>
        <pd:to>JDBC Query</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>endLog</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>errorLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>errorLog</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Render JSON</pd:from>
        <pd:to>endLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>