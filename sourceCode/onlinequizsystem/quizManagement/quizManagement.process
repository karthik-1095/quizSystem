<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.eniquesolution.com/ESI_integartion/CommonFramework" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx="http://www.tibco.com/pe/EngineTypes">
    <pd:name>quizManagement/quizManagement.process</pd:name>
    <pd:startName>HTTP Receiver</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="HTTP Receiver">
        <pd:type>com.tibco.plugin.http.HTTPEventSource</pd:type>
        <pd:resourceType>httppalette.httpEventSource</pd:resourceType>
        <pd:x>160</pd:x>
        <pd:y>66</pd:y>
        <config>
            <outputMode>String</outputMode>
            <defaultEncoding>ISO8859_1</defaultEncoding>
            <inputOutputVersion>5.3.0</inputOutputVersion>
            <newMimeSupport>true</newMimeSupport>
            <sharedChannel>/resources/HTTP Connection.sharedhttp</sharedChannel>
            <Headers>
                <xsd:element name="root">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="Accept" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Accept-Charset" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Accept-Encoding" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Content-Type" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Content-Length" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Connection" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Cookie" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="Pragma" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </Headers>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>620</pd:endX>
    <pd:endY>68</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1680067214007</pd:targetNamespace>
    <pd:activity name="REST Dispatch and Reply">
        <pd:type>com.tibco.plugin.json.rest.server.activities.RestAdapterActivity</pd:type>
        <pd:resourceType>rest.activities.RestAdapter</pd:resourceType>
        <pd:x>388</pd:x>
        <pd:y>70</pd:y>
        <config>
            <eventSource>HTTP Receiver</eventSource>
            <RestService>
                <InnerService>
                    <ns0:application xmlns:ns0="http://wadl.dev.java.net/2009/02" name="Application">
                        <ns0:resources name="quizManagement" base="http://localhost:5323/quizmanagement">
                            <ns0:resource id="createQuestions" path="/createquestions">
                                <ns0:doc/>
                                <ns0:method id="createquestions" name="POST">
                                    <ns0:doc/>
                                    <Binding process="/quizManagement/createQuizQuestions.process">
                                        <RESTResponseHeader>
                                            <xsd:element name="Headers">
                                                <xsd:complexType>
                                                    <xsd:sequence>
                                                        <xsd:element name="StatusLine" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Content-Type" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Location" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Set-Cookie" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Pragma" type="xsd:string" minOccurs="0"/>
                                                    </xsd:sequence>
                                                </xsd:complexType>
                                            </xsd:element>
                                        </RESTResponseHeader>
                                        <Input-Binding xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                                            <xsl:template xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema" match="/*">
                                                <payload>
                                                    <xsl:value-of select="$HTTP-Receiver/ProcessStarterOutput/PostData"/>
                                                </payload>
                                            </xsl:template>
                                        </Input-Binding>
                                        <Output-Binding xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd2">
                                            <xsl:template xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd2" match="/*">
                                                <RESTResponse>
                                                    <xsl:if test="$End/responseMessage">
                                                        <asciiContent>
                                                            <xsl:value-of select="$End/responseMessage"/>
                                                        </asciiContent>
                                                    </xsl:if>
                                                    <Headers>
                                                        <Content-Type>
                                                            <xsl:value-of select="'text/xml'"/>
                                                        </Content-Type>
                                                    </Headers>
                                                </RESTResponse>
                                            </xsl:template>
                                        </Output-Binding>
                                    </Binding>
                                    <ns0:request name="Request">
                                        <ns0:representation mediaType="application/json"/>
                                    </ns0:request>
                                    <ns0:response name="Response0" status="200">
                                        <ns0:representation mediaType="application/xml"/>
                                        <ns0:representation mediaType="application/json"/>
                                    </ns0:response>
                                </ns0:method>
                            </ns0:resource>
                            <ns0:resource id="FetchQuestions" path="/fetchquestions">
                                <ns0:method id="fetchQuestions" name="GET">
                                    <ns0:doc/>
                                    <Binding process="/quizManagement/fetchQuestions.process">
                                        <RESTResponseHeader>
                                            <xsd:element name="Headers">
                                                <xsd:complexType>
                                                    <xsd:sequence>
                                                        <xsd:element name="StatusLine" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Content-Type" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Location" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Set-Cookie" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Pragma" type="xsd:string" minOccurs="0"/>
                                                    </xsd:sequence>
                                                </xsd:complexType>
                                            </xsd:element>
                                        </RESTResponseHeader>
                                        <Output-Binding xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd2">
                                            <xsl:template xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd2" match="/*">
                                                <RESTResponse>
                                                    <asciiContent>
                                                        <xsl:value-of select="tib:render-xml($End/pfx:root, true())"/>
                                                    </asciiContent>
                                                </RESTResponse>
                                            </xsl:template>
                                        </Output-Binding>
                                    </Binding>
                                    <ns0:request name="Request"/>
                                    <ns0:response name="Response0" status="200">
                                        <ns0:representation mediaType="application/xml"/>
                                    </ns0:response>
                                </ns0:method>
                                <ns0:doc/>
                            </ns0:resource>
                            <ns0:resource id="CreateQuiz" path="/createquiz">
                                <ns0:doc/>
                                <ns0:method id="createQuiz" name="POST">
                                    <ns0:doc/>
                                    <Binding process="/quizManagement/quizStructure.process">
                                        <RESTResponseHeader>
                                            <xsd:element name="Headers">
                                                <xsd:complexType>
                                                    <xsd:sequence>
                                                        <xsd:element name="StatusLine" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Content-Type" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Location" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Set-Cookie" type="xsd:string" minOccurs="0"/>
                                                        <xsd:element name="Pragma" type="xsd:string" minOccurs="0"/>
                                                    </xsd:sequence>
                                                </xsd:complexType>
                                            </xsd:element>
                                        </RESTResponseHeader>
                                        <Input-Binding xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                                            <xsl:template xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema" match="/*">
                                                <payload>
                                                    <xsl:value-of select="$HTTP-Receiver/ProcessStarterOutput/PostData"/>
                                                </payload>
                                            </xsl:template>
                                        </Input-Binding>
                                        <Output-Binding xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd3">
                                            <xsl:template xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx="http://www.tibco.com/schemas/onlinequizsystem/schemas/Schema.xsd3">
                                                <RESTResponse>
                                                    <asciiContent>
                                                        <xsl:value-of select="$End/responseMessage"/>
                                                    </asciiContent>
                                                    <Headers>
                                                        <Content-Type>
                                                            <xsl:value-of select="'text/xml'"/>
                                                        </Content-Type>
                                                    </Headers>
                                                </RESTResponse>
                                            </xsl:template>
                                        </Output-Binding>
                                    </Binding>
                                    <ns0:request name="Request">
                                        <ns0:representation mediaType="application/json"/>
                                    </ns0:request>
                                    <ns0:response name="Response0" status="200">
                                        <ns0:representation mediaType="application/xml"/>
                                    </ns0:response>
                                </ns0:method>
                            </ns0:resource>
                        </ns0:resources>
                    </ns0:application>
                </InnerService>
            </RestService>
        </config>
        <pd:inputBindings>
            <ns0:ActivityInputClass xmlns:ns0="http://www.tibco.com/namespaces/tnt/plugins/json">
                <tmp>
                    <xsl:value-of select="$HTTP-Receiver/ProcessStarterOutput/Method"/>
                </tmp>
            </ns0:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="startLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>267</pd:x>
        <pd:y>125</pd:y>
        <config>
            <processName>/processes/infoLog.process</processName>
        </config>
        <pd:inputBindings>
            <ns:LogExceptionHandler>
                <ns:ApplictionName>
                    <xsl:value-of select="'quiz management'"/>
                </ns:ApplictionName>
                <ns:ProcessName>
                    <xsl:value-of select="$_processContext/pfx:ProcessContext/ProjectName"/>
                </ns:ProcessName>
                <ns:LogType>
                    <xsl:value-of select="'start Log'"/>
                </ns:LogType>
                <ns:status>
                    <xsl:value-of select="'success'"/>
                </ns:status>
                <ns:CorrelationID>
                    <xsl:value-of select="''"/>
                </ns:CorrelationID>
                <ns:MessageType>
                    <xsl:value-of select="'request payload'"/>
                </ns:MessageType>
                <ns:Timestamp>
                    <xsl:value-of select="tib:timestamp()"/>
                </ns:Timestamp>
                <ns:PAYLOAD>
                    <xsl:value-of select="concat($HTTP-Receiver/ProcessStarterOutput/PostData,$HTTP-Receiver/ProcessStarterOutput/QueryString)"/>
                </ns:PAYLOAD>
            </ns:LogExceptionHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="endLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>506</pd:x>
        <pd:y>132</pd:y>
        <config>
            <processName>/processes/infoLog.process</processName>
        </config>
        <pd:inputBindings>
            <ns:LogExceptionHandler>
                <ns:ApplictionName>
                    <xsl:value-of select="'quiz management'"/>
                </ns:ApplictionName>
                <ns:ProcessName>
                    <xsl:value-of select="$_processContext/pfx:ProcessContext/ProjectName"/>
                </ns:ProcessName>
                <ns:LogType>
                    <xsl:value-of select="'end log'"/>
                </ns:LogType>
                <ns:status>
                    <xsl:value-of select="'success'"/>
                </ns:status>
                <ns:CorrelationID>
                    <xsl:value-of select="''"/>
                </ns:CorrelationID>
                <ns:MessageType>
                    <xsl:value-of select="'reponse payload'"/>
                </ns:MessageType>
                <ns:Timestamp>
                    <xsl:value-of select="tib:timestamp()"/>
                </ns:Timestamp>
                <ns:PAYLOAD>
                    <xsl:value-of select="tib:render-xml($REST-Dispatch-and-Reply/RESTOutput,true())"/>
                </ns:PAYLOAD>
            </ns:LogExceptionHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>384</pd:x>
        <pd:y>217</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="errorLog">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>520</pd:x>
        <pd:y>220</pd:y>
        <config>
            <processName>/processes/errorLog.process</processName>
        </config>
        <pd:inputBindings>
            <ns:ErrorLogHandler>
                <ns:ApplictionName>
                    <xsl:value-of select="'quiz management'"/>
                </ns:ApplictionName>
                <ns:ProcessName>
                    <xsl:value-of select="$_processContext/pfx:ProcessContext/ProjectName"/>
                </ns:ProcessName>
                <ns:LogType>
                    <xsl:value-of select="'error log'"/>
                </ns:LogType>
                <ns:status>
                    <xsl:value-of select="'failed'"/>
                </ns:status>
                <ns:CorrelationID>
                    <xsl:value-of select="''"/>
                </ns:CorrelationID>
                <ns:MessageType>
                    <xsl:value-of select="'error message'"/>
                </ns:MessageType>
                <ns:Timestamp>
                    <xsl:value-of select="tib:timestamp()"/>
                </ns:Timestamp>
                <xsl:if test="$_error/pfx:ErrorReport/MsgCode">
                    <ns:ErrorCode>
                        <xsl:value-of select="$_error/pfx:ErrorReport/MsgCode"/>
                    </ns:ErrorCode>
                </xsl:if>
                <xsl:if test="$_error/pfx:ErrorReport/Msg">
                    <ns:ErrorMsg>
                        <xsl:value-of select="$_error/pfx:ErrorReport/Msg"/>
                    </ns:ErrorMsg>
                </xsl:if>
            </ns:ErrorLogHandler>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>HTTP Receiver</pd:from>
        <pd:to>startLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>REST Dispatch and Reply</pd:from>
        <pd:to>endLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>startLog</pd:from>
        <pd:to>REST Dispatch and Reply</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>endLog</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>errorLog</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>errorLog</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>